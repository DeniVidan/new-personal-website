{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://new-personal-website-2j6p.onrender.com"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ]
}
